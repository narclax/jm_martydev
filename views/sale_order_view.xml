<odoo>
    <record id="view_order_form_inherit_display_itbis" model="ir.ui.view">
        <field name="name">sale.order.form.inherit.display.itbis</field>
        <field name="model">sale.order</field>
        <field name="inherit_id" ref="sale.view_order_form" />
        <field name="arch" type="xml">
            <xpath expr="//field[@name='validity_date']" position="after">
                <field name="display_ITBIS" />
            </xpath>
        </field>
    </record>
</odoo>